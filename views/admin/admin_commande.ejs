<%- include ../layouts/header.ejs %>
 
<% if (messages.error) { %>
    <p style="color:red"><%- messages.error %></p>
<% } %>
 
<% if (messages.success) { %>
    <p style="color:green"><%- messages.success %></p>
<% } %>

<div class="col-lg-9 col-md-9">
        <% if (data1) { %>
        <% data1.forEach(function(commande){ %>
          <div class="col-lg-2 col-md-2 col-sm-6 col-xs-6 endroit">
              <img src="../../../img/menu_jour/<%= commande.image_plat %>"  class="img-rounded img-responsive ">
              <p style="font-size:15px;"><%= commande.nom_plat %></p>
              <form method="post" action="/admin/commande/<%= commande.id_commande %>">
                <button type="submit" name="Submit" value="Edit" class="btn-xs btn btn-success">
                    terminer</button>
                <input type="hidden" name="_method" value="PUT" />
              </form>
          </div>
        <% }) %>
        <% } %>
          
        
</div>




<%- include ../layouts/footer.ejs %>
